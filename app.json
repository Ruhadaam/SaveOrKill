{
  "scheme": "acme",
  "userInterfaceStyle": "automatic",
  "orientation": "default",
  "ios": {
    "infoPlist": {
      "NSPhotoLibraryUsageDescription": "Uygulama fotoğraf ve video albümlerinizi listelemek için galerinizin erişimine ihtiyaç duyar."
    },
    "bundleIdentifier": "com.ruhadam.myapp"
  },
  "android": {
    "permissions": [
      "READ_MEDIA_IMAGES",
      "READ_MEDIA_VIDEO",
      "READ_EXTERNAL_STORAGE",
      "READ_MEDIA_IMAGES",
      "READ_MEDIA_VIDEO",
      "READ_EXTERNAL_STORAGE"
    ],
    "package": "com.ruhadam.myapp"
  },
  "web": {
    "output": "static"
  },
  "plugins": [
    [
      "expo-router",
      {
        "origin": "https://n"
      }
    ],
    "expo-video",
    "expo-audio"
  ],
  "name": "my-app",
  "slug": "my-app"
}
